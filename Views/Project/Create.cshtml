@using EF_MVC_Repository_UOW.Models;
@model EF_MVC_Repository_UOW.Models.Project


<h2>Add a new Project</h2>
@using (Html.BeginForm("Create", "Project", FormMethod.Post, null))
{

    <table>
        <tr style="height:30px">
            <td>Project Name</td>
            <td width="150px">@Html.EditorFor(x => x.ProjectName)</td>
        </tr>
        <tr style="height:30px">
            <td>Start Date</td>
            <td width="100px">@Html.EditorFor(x => x.StartDate)</td>
        </tr>
        <tr style="height:30px">
            <td>
                End Date
            </td>
            <td width="100px">@Html.EditorFor(x => x.EndDate)</td>
        </tr>
        <tr style="height:30px">
            <td>Customer Name</td>
            <td width="200px">@Html.EditorFor(x => x.CustomerName)</td>
        </tr>
        <tr>
            <td><input type="submit" value="Add Project" /></td>
        </tr>

    </table>
}